program ghostchat;

uses
  Forms,
  source in 'src\source.pas' {ChatForm},
  setting in 'src\setting.pas' {Form1};

{$R *.res}

begin
  Application.Initialize;
  Application.Title := 'Ghost Chat';
  Application.CreateForm(TChatForm, ChatForm);
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
